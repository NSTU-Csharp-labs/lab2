<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:generatorCard="clr-namespace:App.Controls.WorkPlace.GeneratorCard"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="App.Controls.WorkPlace.GeneratorCard.GeneratorCardView">
    <Design.DataContext>
        <generatorCard:GeneratorCardViewModel/>
    </Design.DataContext>
    
    <Button Command="{Binding}">
        <TextBlock FontSize="18" LineHeight="24" Text="{Binding Generator.Name}"/>
    </Button>
</UserControl>
